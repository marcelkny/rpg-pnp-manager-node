<% if(pageNavId !== "login"){ %>
<script type="text/javascript">
  const page = <%- pageNavId %>["id"];

  function pageHighlight() {
    if(page != null){
      let pageIdNavEl = document.getElementById(page);
      if(pageIdNavEl != null){
        pageIdNavEl.classList.add("is-active");
      }
    }
  }
  if(typeof page !== undefined && page !== null){
    pageHighlight();
  }  
</script>
